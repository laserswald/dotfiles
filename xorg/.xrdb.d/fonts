#elif LEMON
URxvt.font: xft:lemon:pixelsize=13
st.font: xft:lemon:pixelsize=13
URxvt.boldFont: xft:lemon:pixelsize=13
st.boldFont: xft:lemon:pixelsize=13

#elif TAMZEN
